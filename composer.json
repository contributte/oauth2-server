{
  "name": "contributte/oauth2-server",
  "description": "An integration of League\\OAuth2Server into Nette framework",
  "type": "library",
  "license": "MIT",
	"require": {
      "php": "^7.3",
      "league/oauth2-server": "^8.1.1",
      "nette/di": "^3.0.4"
	},
	"require-dev": {
    "contributte/psr7-http-message": "^0.6.2",
    "ninjify/nunjuck": "^0.3.0",
    "ninjify/qa": "^0.10.0 || ^0.11",
    "phpstan/phpstan": "^0.12.2",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0"
  },
	"autoload": {
      "psr-4": {
        "Contributte\\OAuth2Server\\": "src"
      }
	},
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
