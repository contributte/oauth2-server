{
  "name": "contributte/oauth2-server",
  "description": "An integration of League\\OAuth2Server into Nette framework",
  "keywords": [
    "nette",
    "contributte",
    "php",
    "oauth2",
    "oauth2 server"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "league/oauth2-server": "^9.1.0",
    "nette/di": "^3.2.2"
  },
  "require-dev": {
    "contributte/phpstan": "^0.2.0",
    "contributte/psr7-http-message": "^0.10.0",
    "contributte/qa": "^0.4.0",
    "contributte/tester": "^0.3.0",
    "nette/application": "^3.2.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\OAuth2Server\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
