{
  "name": "contributte/oauth2-server",
  "description": "An integration of League\\OAuth2Server into Nette framework",
  "keywords": [
    "nette",
    "contributte",
    "php",
    "oauth2",
    "oauth2 server"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "league/oauth2-server": "^8.1.1",
    "nette/di": "^3.0.4"
  },
  "require-dev": {
    "contributte/psr7-http-message": "^0.7",
    "nette/application": "^3.0.6",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\OAuth2Server\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
